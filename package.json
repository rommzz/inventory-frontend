{
  "name": "inventory-frontend",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "./out/main/index.js",
  "scripts": {
    "dev": "vite --port 5555 --host",
    "build": "run-p type-check \"build-only {@}\" --",
